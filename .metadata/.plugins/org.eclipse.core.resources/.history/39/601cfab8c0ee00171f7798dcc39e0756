package third;

import java.awt.Graphics;
import java.awt.Panel;

public class mypanel extends Panel {
  
	int x[] = new int[10];
	int y[] = new int[10];
	char c[] = new char[10]; 
	public void mypanel()
	{
		for(int i = 0;i<10;i++)
		{
			x[i] = (int)(300*Math.random());
			y[i] = (int)(300*Math.random());
			c[i] = (char)(26*Math.random()+97);
		}
	}
	
}
