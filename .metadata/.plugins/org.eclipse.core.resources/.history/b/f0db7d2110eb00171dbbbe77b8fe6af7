package second;

import java.awt.Graphics;
import java.awt.Panel;
import java.awt.event.*;

public class mypanel extends Panel {
   public void paint(Graphics p)
   {
	   p.fillOval(30,30,20,20);
   }
   public void keyPressed()  
   public void run()
   {
	   while(true)
	   {
		   try 
		   {
			   Thread.sleep(100);
		   }
		   catch(Exception e) {}
		   repaint();
	   }
   }
}
