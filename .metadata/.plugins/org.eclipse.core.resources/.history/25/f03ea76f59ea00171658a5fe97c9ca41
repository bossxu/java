    public void paint(Graphics g)
    {
      int x = 30;
       int y = 30;
    	  while(true)
    	  {
    		  g.fillOval(x, y , 20, 20);
    		  y++;
    	  }
    	  public void run() {
    		  
    	  }
    }
}
