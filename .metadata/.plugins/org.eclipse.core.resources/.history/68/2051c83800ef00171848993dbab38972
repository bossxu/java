import javax.swing.JButton;
import javax.swing.JFrame;
import javax.swing.border.Border;

import java.awt.*;

public class Main {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
       JFrame w = new JFrame();
       w.setSize(300,400);
       w.setTitle("尝试");
       
       JButton b1 = new JButton("ok1");
       JButton b2 = new JButton("ok2");
       JButton b3 = new JButton("ok333333333");
       JButton b4 = new JButton("ok4");
       JButton b5 = new JButton("ok5");
       
       w.add(b1,BorderLayout.SOUTH);
       w.add(b2,BorderLayout.NORTH);
       w.add(b3, BorderLayout.WEST);
       w.add(b4,BorderLayout.EAST);
       w.add(b5, BorderLayout.CENTER);
       
       w.setVisible(true);;
       
	}

}
