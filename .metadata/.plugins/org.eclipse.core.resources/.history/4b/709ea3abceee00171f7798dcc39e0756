package forth;

import java.awt.Frame;

public class Main {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
        Frame w = new Frame();
        w.setSize(300, 400);
        Mypanel mp = new Mypanel();
        w.add(mp);
        
        w.addMouseMotionListener(mp);
        mp.addMouseMotionListener(mp);
	}

}
